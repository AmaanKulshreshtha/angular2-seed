{
  "compilerOptions": {
    "target": "ES5",
    "module": "system",
    "moduleResolution": "node",
    "sourceMap": false,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "removeComments": false,
    "noImplicitAny": false
  },
  "filesGlob": [
    "./src/app/**/*.ts"
  ],
  "files": [
    "./src/app/*"
  ],
  "atom": {
    "rewriteTsconfig": true
  }
}